[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = black white bold
  changed = blue white bold
  untracked = red white bold
[user]
  email = daynemay@users.noreply.github.com
[alias]
addnw=!sh -c 'git diff -U0 -w --no-color "$@" | git apply --cached --ignore-whitespace --unidiff-zero -'
	co = checkout

[help]
	autocorrect = 10
[log]
  xxx = format:"%h %h %h"
[pull]
	ff = only
[gpg]
	program = gpg1
