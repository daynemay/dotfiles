# alias ytq="yarn test:quick"

# npm run:XXX commands for the pbsmc-frontend project
alias nrt="npm run test"
alias nrtq="npm run test:quick"

# Create notes in my notes repo 
alias nn="cd ~/projects/notes; ./new_note.sh"
