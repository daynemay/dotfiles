# Git shortcuts
alias gs="git status"
alias gl="git log --oneline"
alias gd="git diff"
alias gds="git diff --staged"
alias gca="git commit --amend"
alias gcane="git commit --amend --no-edit"

# docker-compose is too many letters
alias dcom=docker-compose 

# yarn:XXX commands for the pbsmc-frontend project; 20190910: replaced by nrt, below
